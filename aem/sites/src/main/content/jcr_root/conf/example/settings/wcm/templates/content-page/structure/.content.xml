<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:deviceGroups="[/etc/mobile/groups/responsive]"
        cq:template="/conf/example/settings/wcm/templates/content-page"
        jcr:primaryType="cq:PageContent"
        sling:resourceType="example/sites/components/page">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/responsivegrid">
            <responsivegrid_1409995310
                cq:styleIds="[1545309104202]"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/responsivegrid">
                <image_1442382866
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="core/wcm/components/image/v2/image"
                    altValueFromDAM="true"
                    displayPopupTitle="true"
                    fileReference="/content/dam/example/common/cognifide-logo.png"
                    isDecorative="false"
                    linkURL="/content/example/demo/us/en"
                    titleValueFromDAM="true"/>
