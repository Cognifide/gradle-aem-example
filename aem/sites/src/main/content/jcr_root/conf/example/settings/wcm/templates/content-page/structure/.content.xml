        </root>
        <cq:responsive jcr:primaryType="nt:unstructured">
            <breakpoints jcr:primaryType="nt:unstructured">
                <extrasmall
                    jcr:primaryType="nt:unstructured"
                    title="Extra Small"
                    width="{Long}480"/>
                <small
                    jcr:primaryType="nt:unstructured"
                    title="Small"
                    width="{Long}768"/>
                <medium
                    jcr:primaryType="nt:unstructured"
                    title="Medium"
                    width="{Long}1024"/>
                <large
                    jcr:primaryType="nt:unstructured"
                    title="Large"
                    width="{Long}1280"/>
